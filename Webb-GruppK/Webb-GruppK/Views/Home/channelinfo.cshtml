@model IEnumerable<Webb_GruppK.Models.program>

@{
    ViewBag.Title = "newsinfo";
}

<div id="newsSection">
    @foreach (var item in Model)
    {
        <div id="news">
            <p>
                @item.name,
                @item.description
            </p>
        </div>
    }
</div>




